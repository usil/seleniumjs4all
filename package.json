{
  "name": "selenium4all",
  "version": "1.0.3",
  "description": "",
  "main": "src/index.js",
  "bin": {
    "selenium4all": "./src/index.js",
    "selenium4all-generate-files": "./src/lib/generateRequiredFiles/index.js"
  },
  "scripts": {
    "dev": "node .",
    "test": "jest --coverage --silent && jest-coverage-badges --output './badges'",
    "test:only": "./tests/test.sh"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "src/lib/",
      "src/index.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testTimeout": 15000
  },
  "keywords": [
    "selenium",
    "test",
    "jest"
  ],
  "author": "Diego Ramos",
  "license": "ISC",
  "dependencies": {
    "advanced-settings": "git+https://github.com/usil/advanced-settings.git",
    "archiver": "^5.3.1",
    "chromedriver": "^109.0.0",
    "cli-color": "^2.0.1",
    "cli-table": "^0.3.11",
    "clui": "^0.3.6",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "find-package-json": "^1.2.0",
    "geckodriver": "^2.0.4",
    "jest": "^29.5.0",
    "jest-badge-generator": "^1.1.5",
    "jest-coverage-badges": "^1.1.2",
    "nodemailer": "^6.9.1",
    "package-json-path": "^1.0.9",
    "puppeteer": "^18.1.0",
    "selenium-webdriver": "^4.1.1",
    "smtp-tester": "^2.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.4.0",
    "browser-resolve": "^2.0.0",
    "jest-html-reporter": "^3.4.2"
  }
}
